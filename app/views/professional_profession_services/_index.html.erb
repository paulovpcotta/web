                          <div style="clear:both"></div>
                      </div>
                      <!--/.listing-filter-->
                      <div class="adds-wrapper">
                          <% @professional_profession_services.each do |professional_profession_service| %>
                            <div class="item-list">
                                <div class="col-sm-2 no-padding photobox">
                                    <div class="add-image">
                                        
                                        <%= link_to professional_profession_service do %>
                                          <% if (!professional_profession_service.profile.nil?) %>
                                            <%= image_tag (professional_profession_service.profile.avatar.url(:thumb)), class: 'thumbnail no-margin' %>                                             
                                            <% end %>
                                          <% end %>    
                                      </div>
                                  </div>
                                  <!--/.photobox-->
                                  <div class="col-sm-7 add-desc-box">
                                      <div class="add-details">
                                          <h5 class="add-title"> <%= professional_profession_service.service.name%> </h5>
                                          <span class="info-row">
                                              <span class="add-type business-ads tooltipHere" data-toggle="tooltip" data-placement="right"
                                                    title="Profissional AutÃ´nomo"><i class="fa icon-user-2"></i> </span>
                                              <span class="date"><i class=" icon-person"> </i> <%= User.find(professional_profession_service.user).first_name %> <%= User.find(professional_profession_service.user.id).last_name %> </span>
                                          </span>
                                          <span class="info-row">
                                              <i class=" icon-star" style="color:gold"> </i>
                                              <i class=" icon-star" style="color:gold"> </i>
                                              <i class=" icon-star" style="color:gold"> </i>
                                              <i class=" icon-star-empty"> </i>
                                          </span>
                                      </div>
                                  </div>
                                  <!--/.add-desc-box-->
                                  <div class="col-sm-3 text-right  price-box">
                                      A partir de
                                      <h2 class="item-price"><%= number_to_currency(professional_profession_service.price, unit: "R$",
                                            separator: ",", delimiter: ".") %> </h2>
                                      <a class="btn btn-danger  btn-sm make-favorite">
                                          <i class="fa fa-certificate"></i>
                                          <span>Urgent</span> </a>
                                      <a class="btn btn-default  btn-sm make-favorite"> <i class="fa fa-heart"></i>
                                          <span>Save</span>
                                      </a>
                                  </div>
                                  <!--/.add-desc-box-->
                              </div>
                            <% end %>
                            <!--/.item-list-->


                        </div> <!--/.adds-wrapper-->
                  
