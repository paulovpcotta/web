<h1>New Profile</h1>



<%= form_for(@profile, url: profiles_path, :html => {:multipart => true}) do |f| %>
  <div class="field">
    <%= f.label :Foto %>
    <%= f.file_field :avatar %>
  </div>
  <div class="actions">
    <%= f.submit "Upload" %>
  </div>
<% end %>

<%= link_to 'Back', profiles_path %>
