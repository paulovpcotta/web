<% if not @professional.professional_services.empty? %>
<div class="panel panel-default">
<table class="table">
  <thead>
  <tr>
    <th>Category</th>
    <th>Service</th>
    <th>Price</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @professional.professional_services.each do |ps| %>
      <tr>
        <td><%= ps.service.category.name %></td>
        <td><%= ps.service.name %></td>
        <td><%= ps.price %></td>
        <td><%= link_to 'Show', ps %></td>
        <td><%= link_to 'Edit' %></td>
        <td><%= link_to 'Destroy', ps, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</div>
<%end%>